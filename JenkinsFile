pipeline 
{
    agent any

    
stages{
    stage("Build code")
    {
        steps{
            sh "mvn install"
            sh " ls -lrt target/"

        }
    }

    stage("code analysis")
    {
        steps{

             withSonarQubeEnv('My SonarQube Server') {
                sh 'mvn clean package sonar:sonar'
              }
        }
        
    }
}
}